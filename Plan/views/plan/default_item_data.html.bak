<div class="item_data">
{{try:}}
    {{item_data}}
{{except NameError:}}
    {{item_data = None}}
{{pass}}

{{if item_data is not None:}}
    {{for data in item_data:}}

        <div class="data">
            <div class="data-content">
                <textarea>{{=data.get('data')}}</textarea>
            </div>
            {{if data.get('icons') is not None:}}
                <div class="data-icon-controls">
                    {{for icon in data.get('icons'):}}
                        <i class="{{=icon}}"></i>
                    {{pass}}
                </div>
            {{pass}}
        </div>
    {{pass}}
{{pass}}
</div>
